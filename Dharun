1)public class Main {
    public static void main(String[] args) {
        int a = 4;
        int b = 5;
        int c = 3;
        int d = 2;
        if (a + b > c + d) {
            System.out.println("Sum of a + b is greater than sum of c + d: " + (a + b));
        } else {
            System.out.println("Sum of c + d is greater than or equal to sum of a + b: " + (c + d));
        }
    }
}

2) public class Main {
    public static void main(String[] args) {
        int a = 2;
        if (a % 2 == 0) {
            System.out.println("a is even");
        } else {
            System.out.println("a is odd");
        }
    }
}
3)public class Main {
    public static void main(String[] args) {
        // Loop through characters from 'A' to 'Z'
        for (char ch = 'A'; ch <= 'Z'; ch++) {
            System.out.println(ch);
        }
    }
}
4) public class Main {
    public static void main(String[] args) {
        int a = 2;
        int b = 3;
        int c = a;  // Save the value of a in c
        a = b;      // Assign the value of b to a
        b = c;      // Assign the saved value (original a) to b

        // Print the values of a and b after swapping
        System.out.println("a: " + a);  // Should print 3
        System.out.println("b: " + b);  // Should print 2
    }
}
