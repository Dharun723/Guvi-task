1)public class Statement {
    public static void main(String[] args) {
        int a = 4;
        int b = 5;
        int c = 3;
        int d = 2;
        if (a + b > c + d) {
            System.out.println("Sum of a + b is greater than sum of c + d: " + (a + b));
        } else {
            System.out.println("Sum of c + d is greater than or equal to sum of a + b: " + (c + d));
        }
    }
}

2) public class Statement{
    public static void main(String[] args) {
        int a = 2;
        if (a % 2 == 0) {
            System.out.println("a is even");
        } else {
            System.out.println("a is odd");
        }
    }
}
3)public class Statement {
    public static void main(String[] args) {
        // Loop through characters from 'A' to 'Z'
        for (char ch = 'A'; ch <= 'Z'; ch++) {
            System.out.println(ch);
        }
    }
}
4) public class Statement {
    public static void main(String[] args) {
        int a = 2;
        int b = 3;
        int c = a;  // Save the value of a in c
        a = b;      // Assign the value of b to a
        b = c;      // Assign the saved value (original a) to b

        // Print the values of a and b after swapping
        System.out.println("a: " + a);  // Should print 3
        System.out.println("b: " + b);  // Should print 2
    }
}
5) public class Statement {
    public static void main(String[] args) {
        int number = 29;  // Change this number to test other values
        boolean isPrime = true;
        if (number < 2) {
            isPrime = false;
        } else {
            // Check for factors from 2 to number-1
            for (int i = 2; i <= number / 2; i++) {
                if (number % i == 0) {
                    isPrime = false;
                    break;
                }
            }
}
        if (isPrime) {
            System.out.println(number + " is a prime number.");
        } else {
            System.out.println(number + " is not a prime number.");
        }
    }
}
6) public class Statement {
    public static void main(String[] args) {
        int a = 5;
        
        if (a != 120) {
            System.out.println("It is not equal to 120: " + a);
        } else {
            System.out.println("It is equal to 120: " + a);
        }
    }
}
7) public class Statement{
    public static void main(String[] args) {
        String msg = "guvi geek";
        int c = msg.length();
        System.out.println(c);
    }
}
8) public class Statement {
    public static void main(String[] args) {
        String a = "guvi";  // The string to print
        
        for (int i = 0; i < 10; i++) {  // Loop 10 times
            System.out.println(a);  // Print "guvi" each time
        }
    }
}
9) public class Statment {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
         System.out.print("Enter your age: ");
        int age = scanner.nextInt();
        
        // Check if the person is a senior citizen
        if (age >= 60) {
            System.out.println("You are a senior citizen.");
        } else {
            System.out.println("You are not a senior citizen.");
        }

        scanner.close();
    }
}
